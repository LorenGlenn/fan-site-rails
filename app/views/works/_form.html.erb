<%= render "layouts/errors", :object => @work %>

<%= form_for @work do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :artist %>
  <%= f.text_field :artist %>
  <%= f.label :author %>
  <%= f.text_field :author %>
  <%= f.label :subgenre %>
  <%= f.text_field :subgenre %>
  <%= f.label :image %>
  <%= f.text_field :image %>
  <%= f.submit %>
<% end %>
